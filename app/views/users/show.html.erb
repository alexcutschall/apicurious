Name: <%= @search.user.name %><br>
Image: <%= @search.user.image %><br>
Starred_Url: <%= @search.user.starred_url %><br>
Followers: <%= @search.user.followers %><br>
Following: <%= @search.user.following %><br>
<br><br>

<%= link_to "Create a New Repo", '/repos' %>

Personal Activity:<br>
<% @event_search.events.each do |event| %>
  Type: <%= event.type %><br>
  Display Login: <%= event.display_login %><br>
  Repository Name: <%= event.repo_name %><br>
  <% if event.commit_name != nil %>
    Commit: <%= event.commit_name %><br>
  <% elsif event.pull_request_name != nil %>
    Pull Request: <%= event.pull_request_name %><br>
  <% end %>
  <br>
<% end %>

Followers Activity:<br>
<% @followers_activity.events.each do |event| %>
  Type: <%= event.type %><br>
  Display Login: <%= event.display_login %><br>
  Repository Name: <%= event.repo_name %><br>
  Date: <%= event.date %><br><br>
<% end %>
